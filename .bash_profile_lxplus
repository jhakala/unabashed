# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
	. ~/.bashrc
fi

# User specific environment and startup programs
ANT_HOME=/home/daqowner/ant/bin
PATH=$PATH:$HOME/bin:/home/daqowner/ant/bin:/etc/init.d
export PATH
alias rm="rm -i"
alias ls="ls -G"
bind '"\eOP":"\e\C-e\C-i"'
